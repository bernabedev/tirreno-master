{
    "name": "tirreno/tirreno",
    "type": "project",
    "description": "User security analytics for mission-critical web applications",
    "keywords": [
        "tirreno",
        "fraud-detection",
        "audit-trail",
        "analytics"
    ],
    "license": "AGPL-3.0+",
    "homepage": "https://www.tirreno.com",
    "authors": [
        {
            "name": "Tirreno Technologies SÃ rl",
            "email": "team@tirreno.com",
            "homepage": "https://www.tirreno.com/"
        }
    ],
    "config": {
        "platform": {
            "php": "8"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require": {
        "php": ">=8.0",
        "bcosca/fatfree-core": "3.9.0",
        "phpmailer/phpmailer": "6.9.3",
        "ruler/ruler": "0.4.0",
        "matomo/device-detector": "^6.4.1"
    },
    "extra": {
        "paas": {
            "document-root": "/",
            "index-document": "index.php",
            "nginx-includes": ["nginx.conf"]
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.10.3",
        "phpstan/phpstan": "^2.0"
    },
    "scripts": {
        "compile": [
            "chmod 755 tmp",
            "chmod 755 logs",
            "chmod 640 composer.json",
            "chmod 640 composer.lock",
            "chmod -R 750 config"
        ]
    }
}
